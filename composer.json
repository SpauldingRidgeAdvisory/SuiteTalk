{
    "name": "spauldingridge/suitetalk",
    "description": "Spaulding Ridge LLC NetSuite SuiteTalk package for Laravel",
    "require": {
		"wsdl2phpgenerator/wsdl2phpgenerator": "dev-add-countable-implements"
	}
	"authors": [
        {
            "name": "Dave Weiss",
            "email": "dweiss@spauldingridge.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "SpauldingRidge\\SuiteTalk\\": "src"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "laravel": {
            "providers": [
                "SpauldingRidge\\SuiteTalk\\SuiteTalkServiceProvider"
            ]
        }
    },
	"repositories":[
		{
			"type": "vcs",
			"url": "https://github.com/SpauldingRidgeAdvisory/wsdl2phpgenerator"
		}
	]
}
